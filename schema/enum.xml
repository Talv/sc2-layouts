    <simpleType name="EAnimationEventAction" kind="enum">
        <enumeration value="Play" label="Start playing the animation from its current position."/>
        <enumeration value="Stop" label="Stop playing the animation and reset all values to the frame’s original values."/>
        <enumeration value="Pause" label="Stop playing the animation and hold all values."/>
        <enumeration value="ToggleDirection" label="Flip the direction the animation is playing."/>
        <enumeration value="RefreshBaseValue" label="Resets the *starting value* of a controller. This is normally the original value that is reset to when an animation stops and the value used as the base for relative anchors."/>
        <enumeration value="Reset" label="Set the animation’s time to 0 and direction to forward and sets the original value back onto the frame."/>
        <enumeration value="TogglePause" label="Toggles the paused state of the animation."/>
        <enumeration value="DirectionForward" label="Set the animation’s direction to forward."/>
        <enumeration value="DirectionReverse" label="Set the animation’s direction to reverse."/>
        <enumeration value="Restart" label="Sets the animation's time to 0 and direction to forward."/>
        <enumeration value="Breakpoint" label=""/>
    </simpleType>

    <simpleType name="EAnimationEndAction" kind="enum">
